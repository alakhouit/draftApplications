<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
						http://www.springframework.org/schema/context
						http://www.springframework.org/schema/context/spring-context-4.1.xsd">										

	<bean id="manage" class="com.ilkerkonar.impl.ManageImpl" init-method="init" destroy-method="cleanUp">
		<property name="process" ref="process" />
	</bean>

	<bean id="process" class="com.ilkerkonar.impl.ProcessImpl" init-method="init" destroy-method="cleanUp">
		<constructor-arg index="0" value="ilker" />
	</bean>

</beans>
